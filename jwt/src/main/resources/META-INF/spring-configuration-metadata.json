{
  "groups": [
    {
      "name": "auth.jwt",
      "type": "xyz.catuns.spring.jwt.security.jwt.JwtProperties",
      "sourceType": "xyz.catuns.spring.jwt.security.jwt.JwtProperties"
    },
    {
      "name": "auth.user",
      "type": "xyz.catuns.spring.jwt.config.user.UserConfigurationProperties",
      "sourceType": "xyz.catuns.spring.jwt.config.user.UserConfigurationProperties"
    }
  ],
  "properties": [
    {
      "name": "auth.jwt.secret",
      "type": "java.lang.String",
      "description": "JWT secret."
    },
    {
      "name": "auth.jwt.issuer",
      "type": "java.lang.String",
      "description": "Issuer of the JWT.",
      "defaultValue": "https://catuns.xyz/"
    },
    {
      "name": "auth.jwt.expiration",
      "type": "java.lang.Long",
      "description": "JWT token expiration in ms.",
      "defaultValue": "36000000"
    },
    {
      "name": "auth.jwt.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable JWT autoconfiguration",
      "defaultValue": "true"
    },
    {
      "name": "auth.user.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable creating users",
      "defaultValue": "true"
    },
    {
      "name": "auth.user.users",
      "type": "java.util.List<xyz.catuns.spring.jwt.config.user.UserConfigurationProperties.User>",
      "sourceType": "xyz.catuns.spring.jwt.config.user.UserConfigurationProperties",
      "description": "List of users to create"
    }
  ],
  "hints": [

  ]
}