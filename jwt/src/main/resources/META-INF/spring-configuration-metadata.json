{
  "groups": [
    {
      "name": "jwt",
      "type": "xyz.catuns.spring.jwt.autoconfigure.properties.JwtProperties",
      "sourceType": "xyz.catuns.spring.jwt.autoconfigure.properties.JwtProperties"
    }
  ],
  "properties": [
    {
      "name": "jwt.secret",
      "type": "java.lang.String",
      "description": "JWT secret."
    },
    {
      "name": "jwt.issuer",
      "type": "java.lang.String",
      "description": "Issuer of the JWT.",
      "defaultValue": "https://catuns.xyz/"
    },
    {
      "name": "jwt.expiration",
      "type": "java.time.Duration",
      "description": "JWT token expiration duration.",
      "defaultValue": "10 hours"
    },
    {
      "name": "jwt.refresh",
      "type": "java.time.Duration",
      "description": "Refresh token token duration.",
      "defaultValue": "14 days"
    },
    {
      "name": "jwt.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable JWT autoconfiguration.",
      "defaultValue": "false"
    }
  ],
  "hints": [

  ]
}